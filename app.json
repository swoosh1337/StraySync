{
  "expo": {
    "name": "StraySync",
    "slug": "stray-cat-finder",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/stary-icon-removebg-preview.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/stary-icon-removebg-preview.png",
      "resizeMode": "contain",
      "backgroundColor": "#D0F0C0"
    },
    "assetBundlePatterns": ["**/*"],
    "scheme": "straysync",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.igrigolia.stray",
      "requireFullScreen": true,
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIRequiredDeviceCapabilities": ["armv7", "arm64"],
        "UIDeviceFamily": [1],
        "UISupportedInterfaceOrientations~iphone": ["UIInterfaceOrientationPortrait"],
        "UISupportedInterfaceOrientations": [],
        "UISupportedInterfaceOrientations~ipad": [],
        "LSRequiresIPhoneOS": true,
        "UIRequiresFullScreen": true,
        "NSCameraUsageDescription": "StraySync needs access to your camera to take photos of stray animals you find.",
        "NSPhotoLibraryUsageDescription": "StraySync needs access to your photo library to select photos of stray animals.",
        "NSPhotoLibraryAddUsageDescription": "StraySync needs permission to save photos to your library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/stary-icon-removebg-preview.png",
        "backgroundColor": "#D0F0C0"
      },
      "package": "com.igrigolia.stray",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-location", {
        "locationAlwaysAndWhenInUsePermission": "Allow stray animal Finder to use your location to find nearby cats."
      }],
      ["expo-notifications", {
        "icon": "./assets/stary-icon-removebg-preview.png",
        "color": "#D0F0C0"
      }]
    ],
    "extra": {
      "eas": {
        "projectId": "1ac01b9b-6175-4207-8ff4-811cb8f0a26d"
      }
    }
  }
}